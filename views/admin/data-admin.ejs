<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Data Admin</h1>
    </div>

    <!-- Here is body code -->
    <div class="table-responsive">
        <table class="table table-hover">
            <thead>
                <th style="min-width: 70px;">No.</th>
                <th style="min-width: 300px;">Nama</th>
                <th style="min-width: 400px;">Email</th>
                <th style="min-width: 250px;" class="text-center">Action</th>
            </thead>
            <tbody>
                <% dataAdmin.forEach((data, index) => { index = 1 %>
                    <tr>
                        <td><%= index++ %>.</td>
                        <td><%= data.nama %></td>
                        <td><%= data.email %></td>
                        <td>
                            <a href="<%= data.id %>" class="btn btn-info">Detail</a>
                            <a href="<%= data.id %>" class="btn btn-warning">Ubah</a>
                            <a href="<%= data.id %>" class="btn btn-danger">Delete</a>
                        </td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>

</div>
<!-- /.container-fluid -->

</div>
<!-- End of Main Content -->